<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SalesPro.AddCustomerPage"
             Title="AddCustomerPage">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/> <!--CustID-->
            <RowDefinition Height="*"/> <!--FirstName-->
            <RowDefinition Height="*"/> <!--LastName-->
            <RowDefinition Height="*"/> <!--Address-->
            <RowDefinition Height="*"/> <!--PH Number-->
            <RowDefinition Height="*"/> <!--Date Visited-->
            <RowDefinition Height="*"/> <!--Interested in Product-->
            <RowDefinition Height="*"/> <!--Notes-->
            <RowDefinition Height="*"/> <!--Submit Button-->
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            
        </Grid.ColumnDefinitions>
    </Grid>
</ContentPage>